{
  "name": "rome-portal",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "server": "webpack-dev-server --inline --progress --port 8080 --open",
    "lint": "tslint --force \"src/**/*.ts\"",
    "test": "karma start",
    "test-watch": "karma start --no-single-run --auto-watch",
    "build-production": "rimraf dist && webpack --progress --profile --bail --env.target production",
    "build-staging": "rimraf dist && webpack --progress --profile --bail --env.target staging",
    "build-development": "rimraf dist && webpack --progress --profile --bail --env.minify true --env.target development",
    "build": "npm run build-production",
    "start-regular": "npm run server",
    "start": "npm run server -- --hot"
  },
  "private": true,
